ewarr = list.filter(\r\n          item => item.pinyin.substring(0, 1) === arr[i].toLowerCase()\r\n        );\r\n        //toLowerCase 转为小写的字符串\r\n        // console.log(arr);\r\n        if (!(newarr.length === 0)) {\r\n          newlist.push({\r\n            index: arr[i],\r\n            city: newarr\r\n          });\r\n        }\r\n      }\r\n      console.log(newlist);\r\n      return newlist;\r\n    }\r\n  }\r\n};\r\n</script>"],"sourceRoot":"src/views"}]}